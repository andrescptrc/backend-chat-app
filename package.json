    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.40.0",
    "@typescript-eslint/parser": "^5.40.0",
    "concurrently": "^7.4.0",
    "eslint": "^8.25.0",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.1",
    "jest": "^29.2.1",
    "jest-watch-typeahead": "^2.2.0",
    "prettier": "^2.8.3",
    "prisma": "^4.4.0",
    "tsc-alias": "^1.7.0",
    "typescript": "^4.8.4"
  },
  "dependencies": {
